<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin Panel</title>
</head>
<body>
    <h1>Admin Panel</h1>
    <h2>Tables</h2>
    <% tablesData.forEach(table => { %>
        <h3>Table <%= table.number %></h3>
        <% const complaints = complaintsData.filter(complaint => complaint.table_number === table.number); %>
        <% if (complaints.length > 0) { %>
            <h4>Complaints:</h4>
            <ul>
                <% complaints.forEach(complaint => { %>
                    <li><%= complaint.message %></li>
                <% }); %>
            </ul>
        <% } else { %>
            <p>No complaints for this table.</p>
        <% } %>
    <% }); %>
</body>
</html>
